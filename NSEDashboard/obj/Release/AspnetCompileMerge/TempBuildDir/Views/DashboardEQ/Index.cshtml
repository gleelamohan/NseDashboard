            </header>
        </div>
        <!-- //modal-header-->
        <div class="modal-body" style="padding:0">
            <section class="panel">

                <div class="panel-body">
                    <form class="form-horizontal labelcustomize" data-collabel="3" data-alignlabel="left">
                        <div class="form-group">
                            <label class="control-label col-md-3" style="text-align: left;">Sector</label>
                            <div class="col-md-9">
                                <select class="form-control" ng-model="param.name" ng-options="o as o for o in sector">
                                    <option value="">%</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-3" style="text-align: left;">Name</label>
                            <div class="col-md-9">
                                <input type="text" class="form-control" ng-model="param.fullname" >
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label col-md-3" style="text-align: left;">Diff_52High</label>
                            <div class="col-md-9">
                                <input type="text" class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-3" style="text-align: left;">Diff_52Low</label>
                            <div class="col-md-9">
                                <input type="text" class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
